<div class="container">
<h1>Add a Space</h1>
<%= simple_form_for @space do |f| %>
   <div class="form-inputs"> 
    <%= f.input :title %>
    <%= f.input :address %>
    <%= f.input :price_per_day %>
   </div>
  <div class="form-actions"> 
    <%= f.button :submit, class: "btn btn-primary"%>
   </div>
<% end %>
</div>